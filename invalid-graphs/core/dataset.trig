# ===== Testing invalid Dataset descriptions =====


# Mandatory properties


# ----- Title -----

testgraphs:title-1 {
    test:validDatasetBase
        dct:title "Nynorsktittel 1"@nn, "Nynorsktittel 2"@nn . # too many values in same language
}

testgraphs:title-2 {
    test:validDatasetBase
        dct:title <http://example.com/title> . # wrong type
}

testgraphs:title-3 {
    test:validDatasetBase
        dct:title [ a rdfs:Resource ] . # wrong type
}

testgraphs:title-4 {
    test:validDatasetBase
        dct:title "Title" . # not rdf:langString
}


# ----- Description -----

testgraphs:description-1 {
    test:validDatasetBase
        dct:description "Nynorsktittel 1"@nn, "Nynorsktittel 2"@nn . # too many values in same language
}

testgraphs:description-2 {
    test:validDatasetBase
        dct:description <http://example.com/title> . # wrong type
}

testgraphs:description-3 {
    test:validDatasetBase
        dct:description [ a rdfs:Resource ] . # wrong type
}

testgraphs:description-4 {
    test:validDatasetBase
        dct:description "Description" . # not rdf:langString
}


# ----- Contact Point -----

testgraphs:contactPoint-1 {
    test:validDatasetBase
        dcat:contactPoint "mailto:informasjonsforvaltning@digdir.no" . # wrong type
}

testgraphs:contactPoint-2 {
    test:validDatasetBase
        dcat:contactPoint "http://digdir.no"^^xsd:anyURI . # wrong type
}


# ----- Publisher -----

testgraphs:publisher-1 {
    test:validDatasetBase
        dct:publisher "https://organization-catalog.fellesdatakatalog.digdir.no/organizations/991825827"^^xsd:anyURI . # wrong type
}

testgraphs:publisher-2 {
    test:validDatasetBase
        dct:publisher [ a foaf:Agent ] . # wrong type
}

testgraphs:publisher-3 {
    <http://example.com/dataset> a dcat:Dataset ;
        dct:title "Dataset"@en ;
        dct:description "Dataset description"@en ;
        dcat:contactPoint test:contactPointBase ;
        dcat:theme <http://publications.europa.eu/resource/authority/data-theme/GOVE> ;
        dct:publisher <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/991825827> , # too many values
                      <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/123456789> ;
        .

    <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/123456789> a foaf:Agent . # TODO: should not be necessary, remove
}


# ----- Theme -----

testgraphs:theme-1 {
    test:validDatasetBase
        dcat:theme "http://psi.norge.no/los/tema/apenhet"^^xsd:anyURI . # wrong type
}

testgraphs:theme-2 {
    test:validDatasetBase
        dcat:theme [ a skos:Concept ] . # wrong type
}

