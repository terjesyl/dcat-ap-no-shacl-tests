# ===== Testing valid Dataset descriptions =====


# Mandatory properties
# Title, description, contact point, publisher, theme

testgraphs:all-mandatory-properties-1 {
    test:validDatasetBase a dcat:Dataset .  # From valid base description
}


# ----- Contact Point -----

testgraphs:contactPoint-1 { # several contact points (in addition to the one in base description)
    <http://example.com/another-contactPoint> a vcard:Organization ;
        vcard:fn "A second contact point name"@en ;
        vcard:hasEmail "mailto:example2@example.com" .

    test:validDatasetBase
        dcat:contactPoint <http://example.com/another-contactPoint> ;
        dcat:contactPoint [ a vcard:Organization ;
            vcard:fn "A third contact point name"@en ;
            vcard:hasEmail "mailto:example3@example.com" ;
    ] .
}


# ----- Theme -----

testgraphs:theme-1 {
    test:validDatasetBase
        # several themes not from CV Data Theme, in addition to Data Theme GOVE already part of test:validDatasetBase
        dcat:theme <http://psi.norge.no/los/tema/apenhet> ,
                   <https://wikidata.org/wiki/Q369577> .
}


# Recommended and Optional properties

# ----- Concept (dct:subject) -----

