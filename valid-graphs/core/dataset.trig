PREFIX test: <http://example.com/test-datasets/>
PREFIX testgraphs: <http://example.com/test-graphs/valid/datasets/>

# ===== Testing valid Dataset descriptions =====


# Mandatory properties
# Title, description, contact point, publisher, theme

testgraphs:all-mandatory-properties-1 {
    test:validDatasetBase a dcat:Dataset . # From valid base description
}


# ----- Contact Point -----

testgraphs:contactPoint-1 { # several contact points (in addition to the one in base description)
    <http://example.com/another-contactPoint> a vcard:Organization ;
        vcard:fn "A second contact point name"@en ;
        vcard:hasEmail "mailto:example2@example.com" .

    test:validDatasetBase
        dcat:contactPoint <http://example.com/another-contactPoint> ;
        dcat:contactPoint [ a vcard:Organization ;
            vcard:fn "A third contact point name"@en ;
            vcard:hasEmail "mailto:example3@example.com" ;
    ] .
}


# ----- Theme -----

testgraphs:theme-1 {
    test:validDatasetBase
        # several themes not from CV Data Theme, in addition to Data Theme GOVE already part of test:validDatasetBase
        dcat:theme <http://psi.norge.no/los/tema/apenhet>, <https://wikidata.org/wiki/Q369577> .
}


# Recommended and Optional properties

# ----- Concept (dct:subject) -----

testgraphs:concept-1 {
    test:validDatasetBase
        dct:subject <http://example.com/begrep1>, <http://example.com/begrep2> .
}

testgraphs:concept-2 {
    test:validDatasetBase a dcat:Dataset ;
        dct:subject [ a skos:Concept ] . # blank nodes allowed
}


# ----- Was Generated By / CV Provenance Activity Type -----

testgraphs:wasGeneratedBy-1 {
    test:validDatasetBase
        prov:wasGeneratedBy <https://data.norge.no/vocabulary/provenance-activity-type#administrative-decision>,
                            <https://data.norge.no/vocabulary/provenance-activity-type#collecting-from-third-party> .
}

# ----- Distribution -----

testgraphs:distribution-1 {
    test:validDatasetBase
        dcat:distribution <http://example.com/distribution1>, <http://example.com/distribution2> .
}

testgraphs:distribution-2 {
    test:validDatasetBase
        dcat:distribution [
            a dcat:Distribution ;
            dcat:accessURL <http://example.com/distribution-access-page>
        ] .
}

# ----- Spatial / CV Administrative Units -----
# TODO


# ----- Keyword -----

testgraphs:keyword-1 {
    test:validDatasetBase
        dcat:keyword "keyword 1"@en, "keyword 2"@en, "n√∏kkelord"@nb .
}


# ----- Applicable Legislation -----

testgraphs:applicableLegislation-1 {
    test:validDatasetBase
        dcatap:applicableLegislation <https://lovdata.no/eli/forskrift/2016/12/15/1600>, <https://lovdata.no/eli/lov/2024/12/13/77> .
}


# ----- Temporal -----

testgraphs:temporal-1 {
    test:validDatasetBase
        dct:temporal <http://example.com/temporal-range> .

    <http://example.com/temporal-range> a dct:PeriodOfTime .
}

testgraphs:temporal-2 {
    test:validDatasetBase
        dct:temporal [ a dct:PeriodOfTime ] .
}


# ----- Access Rights / CV Access Rights -----

testgraphs:accessRights-1 {
    test:validDatasetBase
        dct:accessRights <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> .
}


# ----- Qualified Attribution -----

testgraphs:qualifiedAttribution-1 {
    test:validDatasetBase
        prov:qualifiedAttribution [
            a prov:Attribution ;
            prov:agent <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/932384469> ;
            dcat:hadRole <urn:example:isotc211/CI_RoleCode/distributor>
        ] ;
        prov:qualifiedAttribution <http://example.com/qualified-attribution1> .

    <http://example.com/qualified-attribution1> a prov:Attribution ;
        prov:agent <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/972417807> ;
        dcat:hadRole <urn:example:isotc211/CI_RoleCode/distributor> .
}


# ----- Other Identifier -----

testgraphs:otherIdentifier-1 {
    test:validDatasetBase
        adms:identifier [
            a adms:Identifier;
            skos:notation "991825827";
        ] ;
        adms:identifier <http://example.com/other-identifier> .

        <http://example.com/other-identifier> a adms:Identifier ;
            skos:notation "991825827" .
}


# ----- Qualified Relation -----

testgraphs:qualifiedRelation-1 {
    test:validDatasetBase
        dcat:qualifiedRelation [
            a dcat:Relationship ;
            dct:relation <http://example.com/another-dataset1> ;
            dcat:hadRole <http://www.iana.org/assignments/relation/original> ;
        ] ;
        dcat:qualifiedRelation <http://example.com/qualified-relation1> .

        <http://example.com/qualified-relation1> a dcat:Relationship ;
            dct:relation <http://example.com/another-dataset2> ;
            dcat:hadRole <urn:example:isotc211/DS_AssociationTypeCode/stereoMate> .
        
        <http://www.iana.org/assignments/relation/original> a dcat:Role .
        <urn:example:isotc211/DS_AssociationTypeCode/stereoMate> a dcat:Role .
}


# ----- Page -----

testgraphs:page-1 {
    test:validDatasetBase
        foaf:page <http://example.com/dataset-documentation>,
                  <http://example.com/other-dataset-documentation> .
}


# ----- Provenance -----

testgraphs:provenance-1 {
    test:validDatasetBase
        dct:provenance <http://example.com/provenance-statement1> ;
        dct:provenance [ a dct:ProvenanceStatement ] .

    <http://example.com/provenance-statement1> a dct:ProvenanceStatement .
}


# ----- Sample -----

testgraphs:sample-1 {
    test:validDatasetBase
        adms:sample [
            a dcat:Distribution ;
            dcat:accessURL <http://example.com/distribution-access-url1> ;
        ] .
}

testgraphs:sample-2 {
    test:validDatasetBase
        adms:sample <http://example.com/distribution> .

    <http://example.com/distribution> a dcat:Distribution ;
        dcat:accessURL <http://example.com/distribution-access-url2> ;
}


# ----- Modified -----

testgraphs:modified-1 {
    test:validDatasetBase ;
        dct:modified "2025-01-01"^^xsd:date .
}

testgraphs:modified-2 {
    test:validDatasetBase ;
        dct:modified "2025-01-01T12:00:00"^^xsd:dateTime .
}


# ----- Is Referenced By -----

testgraphs:isReferencedBy-1 {
    test:validDatasetBase
        dct:isReferencedBy <http://example.com/publication1> , <http://example.com/publication2> .
}


