# ===== Testing valid usage of Controlled Vocabularies on Dataset descriptions =====


# Mandatory properties

# ----- Theme / CV Data Theme (LEAST 1) -----

testgraphs:dataTheme-1 {
    <http://example.com/dataset1> a dcat:Dataset ;
        dct:title "Base Dataset"@en ;
        dct:description "Base Dataset description"@en ;
        dcat:contactPoint [
            a vcard:Organization ;
            vcard:fn "Base contact point name"@en ;
            vcard:hasEmail "mailto:example@example.com" ;
        ] ;
        dct:publisher <https://organization-catalog.fellesdatakatalog.digdir.no/organizations/991825827> ;
        dcat:theme <http://publications.europa.eu/resource/authority/data-theme/GOVE> ;
        .

    <http://publications.europa.eu/resource/authority/data-theme/GOVE> a skos:Concept ;
        skos:notation "GOVE" ;
        skos:prefLabel "Government"@en ;
        skos:inScheme <http://publications.europa.eu/resource/authority/data-theme> ;
        .
}

testgraphs:dataTheme-2 {
    test:validDatasetBase
        # Valid skos:Concept for GOVE already defined in dataset.base.ttl
        dcat:theme <http://publications.europa.eu/resource/authority/data-theme/GOVE> ,
            <http://example.com/other-themes/theme1> ;
        .

    <http://example.com/other-themes/theme1> a skos:Concept # TODO: clarify, should this be necessary?
}


# Recommended and Optional properties

# ----- Concept (Type Check) -----

testgraphs:concept-1 {
    test:validDatasetBase
        dct:subject <http://example.com/begrep1> .

    <http://example.com/begrep1> a skos:Concept .
}

testgraphs:concept-2 {
    test:validDatasetBase
        dct:subject [ a skos:Concept ] . # blank allowed
}

# ----- Was Generated By / CV Provenance Activity Type -----

# Is a SHOULD requirement,
# there is no check of inScheme and therefore no tests of CV necessary


# ----- Spatial / CV Administrative Units -----
# TODO



# ----- Access Rights / CV Access Rights -----

testgraphs:accessRights-1 {
    test:validDatasetBase
        dct:accessRights <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> .

    <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> a skos:Concept ;
        skos:inScheme  <http://publications.europa.eu/resource/authority/access-right> .
}

# ----- Frequency / CV Frequency -----

testgraphs:frequency-1 {
    test:validDatasetBase
        dct:accrualPeriodicity <http://publications.europa.eu/resource/authority/frequency/ANNUAL> .

    <http://publications.europa.eu/resource/authority/frequency/ANNUAL> a skos:Concept ;
        skos:inScheme <http://publications.europa.eu/resource/authority/frequency> .
}


# ----- Language / CV Language -----

testgraphs:language-1 {
    test:validDatasetBase
        dct:language <http://publications.europa.eu/resource/authority/language/NOR> ;
        .

    <http://publications.europa.eu/resource/authority/language/NOR> a skos:Concept ;
        skos:notation "NOR" ;
        skos:prefLabel "Norwegian"@en ;
        skos:inScheme <http://publications.europa.eu/resource/authority/language> ;
        .
}


# ----- Type / CV Dataset Type -----

testgraphs:type-1 {
    test:validDatasetBase
        dct:type <http://publications.europa.eu/resource/authority/dataset-type/TEST_DATA> ;
        .

    <http://publications.europa.eu/resource/authority/dataset-type/TEST_DATA> a skos:Concept ;
        skos:inScheme <http://publications.europa.eu/resource/authority/dataset-type> ;
        .
}
